<Defs>
	<MemeDef>
		<defName>VME_Structure_Serketist</defName>
		<label>serketism</label>
		<description>There is no higher lifeform than the Hive Queens. Their nourishing jelly is like ambrosia, and the entire world should be remade in their image.</description>
		<category>Structure</category>
		<iconPath>UI/Structures/Structure_Serketism</iconPath>
		<impact>0</impact>
		<worshipRoomLabel>hive</worshipRoomLabel>
		<randomizationSelectionWeightFactor>3</randomizationSelectionWeightFactor>
		<deityCount>0</deityCount>
		<requireOne>
			<li>
				<li>VME_InsectMeatEating_Sacrilegious</li>
				<li>VME_InsectJellyEaten_Loved_Classic</li>
			</li>
		</requireOne>
		<generalRules>
			<rulesStrings>
				<li>memeAdjective->serketic</li>
				<li>memeAdjective->insectoid</li>
				<li>memeAdjective->verminous</li>
				<li>memeLeaderNoun->scarab lord</li>
				<li>memeLeaderNoun->high speaker</li>
				<li>memeLeaderAdjective->buzzing</li>
				<li>memeLeaderAdjective->gregarious</li>
				<li>memeLeaderAdjective->crawling</li>
				<li>memeMoralist->shaman</li>
				<li>memeMoralist->moth priest</li>
				<li>memeMoralist->plague advocate</li>
			</rulesStrings>
		</generalRules>
